{
  "private": true,
  "name": "react-component-design",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "npm run dev -- --open",
    "dev": "vite --host --port=3001",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .tsx,.ts,.jsx,.js",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier src --check",
    "format:fix": "prettier src --write",
    "stylelint": "stylelint \"src/**/*.{css,js,jsx,tsx}\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "test": "vitest --reporter verbose",
    "test:ui": "vitest --ui",
    "test:once": "npm run test -- --run",
    "test:coverage": "vitest run --coverage",
    "test:coverage-watch": "vitest watch --coverage",
    "prepare": "husky install",
    "server": "nodemon server/index.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "src/**/*.css": "npm run stylelint:fix",
    "src/**/*.{tsx,ts,jsx,js}": [
      "npm run lint:fix",
      "npm run format:fix"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@storybook/addon-essentials": "^7.0.0-beta.25",
    "@storybook/addon-interactions": "^7.0.0-beta.25",
    "@storybook/addon-links": "^7.0.0-beta.25",
    "@storybook/blocks": "^7.0.0-beta.25",
    "@storybook/react": "^7.0.0-beta.25",
    "@storybook/react-vite": "^7.0.0-beta.25",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "0.27.0",
    "@vitest/ui": "0.27.0",
    "autoprefixer": "10.4.13",
    "cors": "2.8.5",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.10",
    "eslint-plugin-testing-library": "5.9.1",
    "express": "4.18.2",
    "happy-dom": "8.1.3",
    "husky": "^8.0.0",
    "lint-staged": "13.1.0",
    "nodemon": "2.0.20",
    "postcss": "8.4.21",
    "postcss-import": "15.1.0",
    "postcss-lit": "1.0.1",
    "postcss-nested": "6.0.0",
    "postcss-preset-env": "7.8.3",
    "postcss-styled": "0.34.0",
    "postcss-syntax": "0.36.2",
    "prettier": "2.8.2",
    "prop-types": "15.8.1",
    "storybook": "^7.0.0-beta.25",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-standard": "29.0.0",
    "tailwindcss": "3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-checker": "0.5.3",
    "vitest": "0.27.0"
  }
}